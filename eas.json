{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "package": "me.jagged.planneriti.dev"
      },
      "ios": {
        "bundleIdentifier": "me.jagged.planneriti.dev"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "apk"
      }
    },
    "staging": {
      "channel": "staging"
    }
  },
  "submit": {
    "production": {}
  }
}
